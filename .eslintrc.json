{
    "extends": "standard",
    "root": true,
    "rules": {
        "indent": ["warn", 4],
        "no-extra-semi": "off",
        "semi": "warn",
        "no-trailing-spaces": "off",
        "comma-style": "warn",
        "spaced-comment": "off",
        "no-path-concat": "warn",
        "no-multiple-empty-lines":"off"
    }
}